    {{#icon-eye}}
    {{> mod_mootimeter/elements/snippet_button_icon_only_rounded}}
    {{#js}}
    require(['mod_mootimeter/toggle_teacherpermission'], (module) => module.init(["toggleteacherpermission"]));
    {{/js}}
    {{/icon-eye}}

    {{#icon-check}}
    {{> mod_mootimeter/elements/snippet_button_icon_only_rounded}}
    {{#js}}
    require(['mod_mootimeter/toggle_state'], (module) => module.init(["{{id}}"]));
    {{/js}}
    {{/icon-check}}

